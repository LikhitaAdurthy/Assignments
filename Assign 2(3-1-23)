def diagonalMat(mat):        
    print("Primary Diagonal is: ", end = "")  
    row = len(mat)  
    col = len(mat[0])  
      
    for i in range(row):  
        for j in range(col):  
            if i==j:  
                print(mat[i][j], end = ',')  
              
    print()  
      
  
mat = [[1, 2, 3, 4],  
          [4, 3, 2, 1],  
          [7, 8, 9, 6],  
          [6, 5, 4, 3]]            
diagonalMat(mat)
