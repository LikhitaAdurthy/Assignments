#include <stdio.h> 
#include <string.h> 
 int main ()
 { 
    char str[100],ch;
	gets(str); 
    int len=strlen(str),i,j,k,count=0; 
     for(i=0;i<len;i++) 
	 { 
        for(j=0;j<len-i;j++) 
		{ 
            count++;
            for(k=j;k<j+i+1;k++)
			 { 
                printf("%c",str[k]); 
            } 
            printf("\n"); 
        } 
    } 
    return 0; 
} 
